
<div class="schedule-index-header">
<h2>Workout History for: <%= user.name %></h2>
</div>

<ul class="schedule-index-nav">
    <li class="left"><a href="/exercise">Home</a></li>
    <li class="right"><a href="/logout">Logout</a></li>
  </ul>

<% schedule.forEach(schedule => { %>
        <div>
            <p><b>Schedule Name:</b> <%= schedule.scheduleName%> </p>
            <p><b>Schedule Date:</b> <%= schedule.date%> </p>
            <p><b>Reps:</b> <%= schedule.reps%> </p>
            <p><b>Sets:</b> <%= schedule.sets%> </p>
        </div>

        <p><b>Exercise Name:</b> <%= schedule.exerciseId.name %> </p>
        <p><b>Exercise Description:</b> <%= schedule.exerciseId.description %> </p>
        <p><img src="<%= schedule.exerciseId.image %>" width="100" height="100"> </p>

        <form action="/schedule/<%= schedule.id%>/edit" >
            <button type="submit">Edit Schedule</button> 
        </form>

        <form action="/schedule/<%= schedule.id%>?_method=DELETE" method="POST">
            <button type="submit">Delete Schedule</button>
        </form>

        
        <hr>
<% }) %>






